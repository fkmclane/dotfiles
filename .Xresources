! URxvt
URxvt.font: xft:Monofur:size=14
URxvt.boldFont: xft:Monofur:size=14
URxvt.scrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.secondaryWheel: true
URxvt.perl-ext-common: font-size
URxvt.keysym.C-equal: perl:font-size:increase
URxvt.keysym.C-minus: perl:font-size:decrease

! rofi
rofi.font: Monofur 14
!                  bg       border
rofi.color-window: #121212, #aa4444
!                  bg       fg       bgalt    hlbg     hlfg
rofi.color-active: #121212, #a9a9a9, #1a1a1a, #535551, #d7d7d7
rofi.color-normal: #121212, #a9a9a9, #1a1a1a, #535551, #d7d7d7
rofi.color-urgent: #121212, #ea2828, #1a1a1a, #535551, #d7d7d7

