polkit.addRule(function(action, subject) {
    if ((((action.id == "org.freedesktop.systemd1.manage-units") && (action.lookup("unit") == "openvpn-client@airvpn.service")) || ((action.id == "org.freedesktop.policykit.exec") && ((action.lookup("command_line") == "/sbin/modprobe tun") || (action.lookup("command_line") == "/sbin/modprobe -r tun")))) && subject.isInGroup("wheel")) {
        return polkit.Result.YES;
    }
});
